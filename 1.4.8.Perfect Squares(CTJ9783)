package q9783;

public class CTJ9783 {

    public static int findTheIndices(int n) {
        // Write your code here
        int count = 0;
        for (int i = 1; i * i < n; i++) {
            count++;
        }
        return count;
    }

    public static void main(String[] args) {
        int n = Integer.parseInt(args[0]);
        System.out.println(findTheIndices(n));
    }
}
